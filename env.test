# Variáveis de Ambiente - Teste
# Arquivo: .env.test

# API Configuration
VITE_API_BASE_URL=https://apidev.coppetec.ufrj.br/EXTRATO
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=500

# Environment
VITE_NODE_ENV=test
VITE_APP_ENV=test

# Features
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_PERFORMANCE_MONITORING=false

# WebSocket (para atualizações em tempo real)
VITE_WEBSOCKET_URL=https://apidev.coppetec.ufrj.br/EXTRATO

# Analytics e Monitoramento
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# Cache e Performance
VITE_CACHE_TTL=300000
VITE_ENABLE_SERVICE_WORKER=false

# Testes
VITE_ENABLE_TEST_MODE=true
VITE_MOCK_API_DELAY=100

# Segurança
VITE_ENABLE_HTTPS_ONLY=true
VITE_ENABLE_CSP=false

# Logs
VITE_LOG_LEVEL=debug
VITE_ENABLE_ERROR_REPORTING=false
